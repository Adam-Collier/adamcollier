{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "unocss './app/**/*.{ts,tsx}' './app/utils/unified/**/*.js' -o ./app/styles/uno.css",
    "dev": "remix build && run-p dev:*",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public",
    "dev:css": "unocss './app/**/*.{ts,tsx}' './app/utils/unified/**/*.js' -o ./app/styles/uno.css --watch",
    "dev:db": "pscale connect adamcollier dev --port 3309",
    "db:prod": "pscale connect adamcollier main --port 3309",
    "db:dev": "pscale connect adamcollier dev --port 3309",
    "db:shadow": "pscale connect adamcollier shadow --port 3310"
  },
  "dependencies": {
    "@notionhq/client": "^0.4.11",
    "@prisma/client": "^3.7.0",
    "@remix-run/cloudflare": "^1.4.3",
    "@remix-run/cloudflare-pages": "^1.4.3",
    "@remix-run/react": "^1.4.3",
    "@unocss/reset": "^0.17.3",
    "hast-util-to-string": "^1.0.4",
    "html5parser": "^2.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "refractor": "^3.3.1",
    "rehype-external-links": "^1.0.0",
    "rehype-highlight": "^4.1.0",
    "rehype-parse": "^7.0.1",
    "rehype-remark": "^8.1.1",
    "rehype-sanitize": "^4.0.0",
    "rehype-stringify": "^8.0.0",
    "remark-external-links": "^8.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.1.0",
    "remark-stringify": "^9.0.1",
    "rss-parser": "^3.12.0",
    "sharp": "^0.30.1",
    "swr": "^1.1.1",
    "unified": "^9.2.1",
    "unist-util-visit": "^2.0.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@iconify-json/ri": "^1.0.2",
    "@remix-run/dev": "^1.4.3",
    "@remix-run/eslint-config": "^1.4.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/sharp": "^0.29.5",
    "eslint": "^8.11.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.7.0",
    "typescript": "^4.1.2",
    "unocss": "^0.25.0",
    "wrangler": "beta"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
