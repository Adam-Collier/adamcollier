{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "unocss './app/**/*.{ts,tsx}' './app/utils/unified/**/*.js' -o ./app/styles/uno.css",
    "dev": "node -r dotenv/config node_modules/.bin/remix dev",
    "dev:css": "unocss './app/**/*.{ts,tsx}' './app/utils/unified/**/*.js' -o ./app/styles/uno.css --watch",
    "postinstall": "remix setup node",
    "db:prod": "pscale connect adamcollier main --port 3309",
    "db:dev": "pscale connect adamcollier dev --port 3309",
    "db:shadow": "pscale connect adamcollier shadow --port 3310"
  },
  "dependencies": {
    "@notionhq/client": "^0.4.11",
    "@prisma/client": "^3.7.0",
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "@remix-run/vercel": "^1.1.1",
    "@unocss/reset": "^0.17.3",
    "hast-util-to-string": "^1.0.4",
    "html5parser": "^2.0.2",
    "hybrid-disk-cache": "^0.2.0",
    "lru-cache": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "refractor": "^3.3.1",
    "rehype-external-links": "^1.0.0",
    "rehype-highlight": "^4.1.0",
    "rehype-parse": "^7.0.1",
    "rehype-remark": "^8.1.1",
    "rehype-sanitize": "^4.0.0",
    "rehype-stringify": "^8.0.0",
    "remark-external-links": "^8.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.1.0",
    "remark-stringify": "^9.0.1",
    "remix": "^1.1.1",
    "remix-image": "^0.3.10",
    "rss-parser": "^3.12.0",
    "swr": "^1.1.1",
    "unified": "^9.2.1",
    "unist-util-visit": "^2.0.3"
  },
  "devDependencies": {
    "@iconify-json/ri": "^1.0.2",
    "@remix-run/dev": "^1.1.1",
    "@types/lru-cache": "^5.1.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "dotenv": "^10.0.0",
    "prisma": "^3.7.0",
    "typescript": "^4.1.2",
    "unocss": "^0.25.0"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
